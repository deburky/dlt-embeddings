# dlt Secrets File
# This file contains sensitive credentials and should NOT be committed to git
# Copy this file to secrets.toml and fill in your actual credentials

[destination.redshift.credentials]
database = "your_database"
password = "your_password"
username = "your_username"
host = "your-redshift-cluster.region.redshift.amazonaws.com"
port = 5439
# Explicitly set the schema to use
schema = "your_schema"

# AWS Credentials (for S3 access)
[sources.aws]
aws_access_key_id = "your_aws_access_key_id"
aws_secret_access_key = "your_aws_secret_access_key"
region_name = "us-east-1"

# S3 Configuration
[sources.s3]
bucket_name = "your-s3-bucket-name"
prefix = "log/"
